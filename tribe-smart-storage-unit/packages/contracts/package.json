{
  "name": "contracts",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "mud build",
    "clean": "forge clean && rimraf src/codegen",
    "deploy:local": "pnpm run build && mud deploy --profile=local --worldAddress ${WORLD_ADDRESS:-$1}",
    "deploy:garnet": "pnpm run build && mud deploy --profile=garnet --worldAddress 0x7fe660995b0c59b6975d5d59973e2668af6bb9c5",
    "deploy:redstone": "pnpm run build && mud deploy --profile=redstone --worldAddress ${WORLD_ADDRESS:-$1}",
    "dev": "pnpm mud dev-contracts",
    "lint": "pnpm run prettier && pnpm run solhint",
    "prettier": "prettier --write 'src/**/*.sol'",
    "solhint": "solhint --config ./.solhint.json 'src/**/*.sol' --fix",
    "test:deploy": "tsc --noEmit && mud test",
    "test": ". ./.env && forge test --fork-url $RPC_URL",
    "mock-data": ". ./.env && pnpm forge script ./script/MockSsuData.s.sol:MockSsuData --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vvv",
    "interact": ". ./.env && pnpm forge script ./script/Interact.s.sol:Interact --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vv",
    "postdeploy": ". ./.env && pnpm forge script ./script/PostDeploy.s.sol:PostDeploy --broadcast --rpc-url $RPC_URL --chain-id $CHAIN_ID --sig \"run(address)\" $WORLD_ADDRESS -vv",
    "env-stillness": "bash ../../shell-scripts/ConvertEnvToServer.sh stillness",
    "env-nova": "bash ../../shell-scripts/ConvertEnvToServer.sh nova",
    "env-local": "bash ../../shell-scripts/ConvertEnvToLocal.sh"
  },
  "dependencies": {
    "@eveworld/common-constants": "0.0.13",
    "@eveworld/world": "0.0.17",
    "@latticexyz/cli": "2.2.14",
    "@latticexyz/explorer": "2.2.14",
    "@latticexyz/schema-type": "2.2.14",
    "@latticexyz/store": "2.2.14",
    "@latticexyz/world": "2.2.14",
    "@latticexyz/world-modules": "2.2.14",
    "i": "^0.3.7",
    "npm": "^10.8.0"
  },
  "devDependencies": {
    "@types/node": "^18.15.11",
    "ds-test": "https://github.com/dapphub/ds-test.git#e282159d5170298eb2455a6c05280ab5a73a4ef0",
    "forge-std": "https://github.com/foundry-rs/forge-std.git#74cfb77e308dd188d2f58864aaf44963ae6b88b1",
    "prettier": "^2.6.2",
    "prettier-plugin-solidity": "1.1.3",
    "solhint": "^3.3.7",
    "solhint-config-mud": "2.2.9",
    "solhint-plugin-mud": "2.2.9"
  }
}